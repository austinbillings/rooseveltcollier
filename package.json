{
  "name": "roosvelt-collier-site",
  "version": "0.1.0",
  "scripts": {
    "linux-deps": "apt-get install -y xvfb x11-xkb-utils xfonts-100dpi xfonts-75dpi xfonts-scalable xfonts-cyrillic x11-apps clang libdbus-1-dev libgtk2.0-dev libnotify-dev libgnome-keyring-dev libgconf2-dev libasound2-dev libcap-dev libcups2-dev libxtst-dev libxss1 libnss3-dev gcc-multilib g++-multilib",
    "clean": "rm -rf ./dist/*",
    "compile": "NODE_ENV=production webpack",
    "build": "npm run clean && npm run compile",
    "wds": "webpack-dev-server --content-base . --hot --inline",
    "dev": "atom . && npm run wds",
    "start": "xvfb-run -a node services/index.js",
    "update-tour": "xvfb-run -a node services/tour-fetcher/invoke.js"
  },
  "main": "index.html",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.2",
    "deep-equal": "^1.0.1",
    "express": "^4.15.4",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.7.2",
    "jquery": "^3.3.1",
    "mansion-ui": "^2.5.3",
    "moment": "^2.18.1",
    "nightmare": "^3.0.1",
    "node-sass": "^4.9.2",
    "opn": "^5.3.0",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scrollchor": "^3.0.0",
    "react-youtube": "^7.4.0",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5",
    "xx": "https://github.com/austinbillings/xx.git",
    "zaq": "^2.1.2"
  }
}
